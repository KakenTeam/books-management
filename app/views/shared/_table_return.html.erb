<table class="table table-hover">
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Due date</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% object.each do |e| %>
      <tr>
        <td><%= Book.find_by(id: e.book_id).title %></td>
        <td><%= Book.find_by(id: e.book_id).author %></td>
        <td><%= e.due_date %></td>
        <%= render 'shared/return', object: e %>
      </tr>
    <% end %>
  </tbody>
</table>